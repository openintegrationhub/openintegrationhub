{
  "name": "snapshots-service",
  "version": "0.3.2",
  "description": "Service for managing flow snapshots",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs"
  },
  "engines": {
    "node": ">= 12"
  },
  "scripts": {
    "start": "node dist/app.js",
    "start:dev": "NODE_ENV=development ts-node-dev -r dotenv/config ./src/app.ts | bunyan",
    "start:container": "ts-node-dev ./src/app.ts | bunyan",
    "test": "NODE_ENV=test mocha --exit --require ts-node/register src/**/*.spec.ts",
    "build:ts": "tsc",
    "build:docker": "docker build -t openintegrationhub/snapshots-service:${VERSION} -f Dockerfile ../../"
  },
  "license": "UNLICENSED",
  "devDependencies": {
    "@tsconfig/node14": "1.0.1",
    "@types/bunyan": "1.8.7",
    "@types/chai": "4.1.7",
    "@types/koa-bunyan-logger": "2.1.3",
    "@types/koa-qs": "2.0.0",
    "@types/koa-router": "7.0.42",
    "@types/lodash": "4.14.169",
    "@types/mocha": "5.2.7",
    "@types/nock": "10.0.3",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "chai": "4.2.0",
    "dotenv": "8.2.0",
    "eslint": "7.32.0",
    "mocha": "9.1.1",
    "nock": "10.0.6",
    "supertest": "4.0.2",
    "ts-node": "8.0.2",
    "ts-node-dev": "1.1.8",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "@openintegrationhub/iam-utils": "*",
    "backend-commons-lib": "*",
    "bunyan": "1.8.12",
    "koa": "2.7.0",
    "koa-bodyparser": "4.2.1",
    "koa-bunyan-logger": "2.1.0",
    "koa-qs": "3.0.0",
    "koa-router": "7.4.0",
    "koa2-swagger-ui": "5.1.0",
    "lodash": "4.17.21",
    "mongoose": "5.13.13"
  }
}
