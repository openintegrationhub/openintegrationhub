{
  "name": "analytics-service",
  "version": "0.0.5",
  "description": "Stores provenance events and other analytics related data.",
  "main": "index.js",
  "scripts": {
    "start": "node app/index.js",
    "start:nodemon": "NODE_ENV=development nodemon -r dotenv/config app/index.js --watch app/ | bunyan",
    "start:container": "nodemon -r dotenv/config app/index.js --watch app/ | bunyan",
    "start:nodemon:minikube": ". ../../minikube/.env && npm run start:nodemon",
    "pretest": "eslint app test --ext .js",
    "test": "jest --runInBand",
    "lint_fix": "eslint app test --fix",
    "build": "echo \"No Build defined\"",
    "build:docker": "docker build -t openintegrationhub/analytics-service:${VERSION} -f Dockerfile ../../"
  },
  "dependencies": {
    "@openintegrationhub/event-bus": "*",
    "@openintegrationhub/iam-utils": "*",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.14",
    "bunyan-format": "^0.2.1",
    "cors": "^2.8.5",
    "cronstrue": "^1.94.0",
    "dayjs": "^1.11.3",
    "dayjs-plugin-utc": "^0.1.2",
    "express": "^4.17.1",
    "mongoose": "6.2.1",
    "node-fetch": "^2.6.6",
    "node-schedule": "^2.1.0",
    "qs": "^6.10.3",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-mocha": "9.0.0",
    "jest": "26.6.0",
    "jest-environment-node": "^26.1.0",
    "mongod": "^2.0.0",
    "mongodb": "^3.5.9",
    "mongodb-memory-server": "7.4.0",
    "nock": "^13.0.2",
    "nodemon": "2.0.4",
    "path": "^0.12.7",
    "supertest": "^4.0.2"
  },
  "author": "hschmidt",
  "license": "Apache-2.0"
}
