              "application/json" : {
                "schema" : {
                  "properties" : {
                    "data" : {
                      "$ref" : "#/components/schemas/MutableAuthClient"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "$ref": "#/components/responses/401Unauthorized"
          }
        }
      },
      "patch" : {
        "tags" : [ "Auth clients" ],
        "summary" : "Update a client",
        "description" : "Update a client",
        "operationId" : "updateClient",
        "parameters" : [ {
          "name" : "clientId",
          "in" : "path",
          "description" : "ID of the client to update",
          "required" : true,
          "style" : "simple",
          "explode" : false,
          "schema" : {
            "type" : "string"
