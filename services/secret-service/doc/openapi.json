          "accessToken" : {
            "type" : "string"
          },
          "accessTokenSecret" : {
            "type" : "string"
          },
          "scope": {
            "type": "string"
          },
          "expires": {
            "type": "string"
          },
          "externalId": {
            "type": "string"
          }
        }
      },
      "OA2AuthorizationCodeSecret" : {
        "required" : ["authClientId", "accessToken", "expires"],
