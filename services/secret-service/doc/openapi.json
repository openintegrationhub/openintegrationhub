      "ApiKeySecret": {
        "required": ["key"],
        "type": "object",
        "properties": {
          "key": {
            "type": "string"
          },
          "headerName": {
            "type": "string"
          }
        }
      },
      "SessionAuthSecret": {
        "required": ["authClientId"],
        "type": "object",
        "properties": {
          "authClientId": {
            "type": "string"
          },
          "accessToken": {
            "type": "string"
          },
          "inputFields": {}
        }
      },
      "OA1TwoLeggedSecret": {
        "type": "object",
