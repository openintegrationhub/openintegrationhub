            "type": "string",
            "example": "61708255c0587c002c49f68a"
          },
          "name": {
            "type": "string",
            "example": "My Simple Secret"
          },
          "type": {
            "type": "string",
            "example": "SIMPLE"
          },
          "owners": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Owner"
            }
          }
        }
